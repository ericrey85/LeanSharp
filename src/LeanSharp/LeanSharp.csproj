<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <PackageId>LeanSharp</PackageId>
    <version>1.0.11</version>
    <Authors>Eric Rey</Authors>
    <Description>
      LeanSharp allows you to write code in a more declarative and functional way in C#. By using LeanSharp, a lot of boilerplate and imperative code can be removed from your codebase.
    </Description>
    <RepositoryUrl>https://github.com/ericrey85/LeanSharp</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageTags>FunctionalCSharp DeclarativeCSharp FunctionalProgramming</PackageTags>
  </PropertyGroup>

</Project>
